<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Weather_App.Search"
        Title="Search">
        <Grid>
                <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="*"/>
                </Grid.RowDefinitions>

                <TextBox x:Name="searchBox" Grid.Row="0" Margin="10" />
                <Button x:Name="searchButton" Grid.Row="0" Margin="10" Content="Search" Click="SearchButton_Click"/>

                <ListBox x:Name="resultsBox" Grid.Row="1" Margin="10"/>
        </Grid>
</Window>
